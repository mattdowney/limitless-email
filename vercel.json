{
  "version": 2,
  "name": "limitless-email",
  "alias": ["limitless.email"],
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/blog/(.*)",
      "dest": "/blog/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ]
}